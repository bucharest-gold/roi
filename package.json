{
  "name": "roi",
  "version": "0.14.0",
  "description": "A basic and fast REST http-client library.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/roi-test.js | tap-spec",
    "lint": "eslint test/*.js index.js benchmark/*.js",
    "benchmark": "node benchmark/benchmark.js",
    "profile": "node benchmark/profile.js",
    "prepublish": "nsp check",
    "coverage": "istanbul cover tape test/roi-test.js",
    "docs": "jsdoc --verbose -d docs -t ./node_modules/ink-docstrap/template -R README.md index.js",
    "dependencyCheck": "szero . --ci"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "index.js"
  ],
  "keywords": [
    "http",
    "promises",
    "node",
    "requests"
  ],
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "bench": "^0.3.6",
    "eslint": "^3.8.1",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "genet": "0.0.5",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.2",
    "json-server": "0.8.14",
    "lodash": "^4.15.0",
    "node-fetch": "^1.6.3",
    "nsp": "^2.6.1",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "wreck": "^10.0.0"
  },
  "homepage": "https://github.com/bucharest-gold/roi#readme",
  "repository": {
    "type": "git",
    "url": "bucharest-gold/roi"
  },
  "bugs": "https://github.com/bucharest-gold/roi/issues",
  "dependencies": {
    "fidelity": "^4.0.0"
  }
